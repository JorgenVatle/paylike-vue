{
  "name": "paylike-vue",
  "version": "1.0.2",
  "description": "Vue wrapper for the Paylike client SDK",
  "main": "dist/paylike-vue.umd.js",
  "unpkg": "dist/paylike-vue.umd.js",
  "module": "dist/paylike-vue.esm.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:umd && npm run build:es && npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/paylike-vue.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/paylike-vue.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/paylike-vue.min.js",
    "preversion": "git pull && npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JorgenVatle/paylike-vue.git"
  },
  "keywords": [
    "Paylike SDK",
    "Paylike",
    "Vue"
  ],
  "author": "JÃ¸rgen Vatle <Jorgen@WcAServices.net> (https://github.com/JorgenVatle)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JorgenVatle/paylike-vue/issues"
  },
  "homepage": "https://github.com/JorgenVatle/paylike-vue#readme",
  "devDependencies": {
    "rollup": "^0.66.6",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "tslib": "^1.9.3",
    "typescript": "^3.1.3",
    "vue-template-compiler": "^2.5.17",
    "vuetype": "^0.3.2"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-plugin-load-script": "^1.1.0"
  }
}
